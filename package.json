{
  "name": "apifox2ts",
  "version": "0.3.0",
  "description": "apifox to typescript",
  "keywords": [
    "apifox",
    "typescript",
    "openapi",
    "generator"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./request": {
      "import": "./dist/request.mjs",
      "require": "./dist/request.js",
      "types": "./dist/request.d.ts"
    }
  },
  "bin": {
    "apifox2ts": "dist/cli.js"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "dev": "tsup --watch",
    "build": "tsup --clean",
    "publish": "bumpp"
  },
  "dependencies": {
    "unconfig": "^0.3.10"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^2.4.4",
    "@types/node": "^20.4.4",
    "bumpp": "^10.2.0",
    "eslint": "^8.25.0",
    "eslint-define-config": "^1.7.0",
    "prettier": "^2.7.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
